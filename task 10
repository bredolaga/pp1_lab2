#include <iostream>
using namespace std;

int main () {
    int a, b, c, d ,f ,g;
    cin >> a;
    cin >> b;
    cin >> c;
    d = a + b;
    f = a + c;
    g = b + c;
   if (d > c && f > b && g > a) {
       cout << "Yes" << endl;
   }
    else {
        cout << "No" << endl;
    }
    return 0;
}
